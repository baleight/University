.globl main

.data
conta: .string "i:"
endline: .string "\n"
.text
main:
li t1, 10 #n
li t0, 0 #i


loop:
beq t0, t1 exit

#lw t3, a0, x0 no
#lw t4, endline, x0 no
li a7, 4
la a0, conta
ecall

#add a0, a0 ,t0
#addi a7, a7, 1
li a7, 1
mv a0, t0
ecall

li a7, 4
la a0, endline
ecall

addi t0,t0 1
j loop

exit:
addi a0,a0,0
jr ra